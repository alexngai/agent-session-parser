{
  "name": "trajectory-parser",
  "version": "0.1.0",
  "description": "Standalone utilities for parsing agent session transcripts (Claude Code, Gemini CLI, and more)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "transcript",
    "parser",
    "claude",
    "gemini",
    "agent",
    "session",
    "trajectory"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
